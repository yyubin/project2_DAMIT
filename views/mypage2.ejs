				<script>
					function doDisplay(what) {


						if (what == 1) {
							var change = document.getElementById('chn_pw');
							change.style.display = 'block';
						} else if (what == 2) {
							var change = document.getElementById('chn_phone');
							change.style.display = 'block';
						}
						else if (what == 3) {
							var change = document.getElementById('chn_com');
							change.style.display = 'block';
						}
						else if (what == 4) {
							var change = document.getElementById('chn_dept');
							change.style.display = 'block';
						}
						else {
							var change = document.getElementById('chn_req');
							change.style.display = 'block';
						}


					}
				</script>

