				<script>
					function sel(seq) {

						$.ajax({
							type: "POST",
							url: "/mypage2_label",
							data: { "seq": seq },
							dataType: "JSON",
							success: function (data) {
								
								inner(data);
							}
						})

					};
					function inner(data) {
						let html = '<p><b>라벨링 순번</b>     ' + data.sel_seq + '</p>' +
							'<p><b>의뢰자 ID</b>     ' + data.sel_id + '</p>' +
							'<p><b>회사</b>     ' + data.sel_company + '    <u onclick="doDisplay(3)">수정</u></p>' +
							'<div id="chn_com" style="display: none;"><input type="text" placeholder="변경하실 회사명을 입력하세요." class="pass" id="modi_com"></div>' +
							'<p><b>부서</b>     ' + data.sel_dept + '    <u onclick="doDisplay(4)">수정</u></p>' +
							'<div id="chn_dept" style="display: none;"><input type="text" placeholder="변경하실 부서명을 입력하세요." class="pass" id="modi_dept"></div>' +
							'<p><b>요구사항</b>     ' + data.sel_req + '    <u onclick="doDisplay(5)">수정</u></p>' +
							'<div id="chn_req" style="display: none;"><input type="text" placeholder="변경하실 요구사항을 입력하세요." class="pass" id="modi_req"></div>' +
							'<p><b>데이터 타입</b>     ' + data.sel_data + '</p>' +
							'<p><b>의뢰 날짜</b>     ' + data.sel_date + '</p>' +
							'<div style="text-align: center;"><input type="submit" class="genric-btn primary" value="수정 완료"></div>';
						document.getElementById('result').innerHTML = html;
					}


				</script>

